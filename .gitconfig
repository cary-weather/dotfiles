[user]
	email = carymrobbins@gmail.com
	name = Cary M. Robbins
[push]
	default = current
[pull]
	default = current
[merge]
	tool = kdiff3
[alias]
	co = checkout
	br = branch
	a = add
	ap = add -p
	ci = commit
	s = show
	st = status
	d = diff
	l = log
	f = fetch
	unstage = reset HEAD --
	autosquash = rebase -i --autosquash
	as = rebase -i --autosquash
	p = push
	pf = push --force-with-lease
        rh = rebase-helper
        rho = rebase-helper origin/master
[core]
	commentchar = "#"
	excludesfile = ~/.gitignore
	pager = less -F -X
