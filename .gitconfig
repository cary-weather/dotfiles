[user]
	email = carymrobbins@gmail.com
	name = Cary M. Robbins
[push]
	default = current
[pull]
	default = current
[merge]
	tool = kdiff3
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	autosquash = rebase -i --autosquash
	pf = push --force-with-lease
[core]
	commentchar = "#"
	excludesfile = ~/.gitignore
	pager = less -F -X
